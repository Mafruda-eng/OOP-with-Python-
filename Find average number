class Student:

     def __init__(self,name,math, phy, che):
         self.name = name
         self.math = math
         self.phy = phy
         self.che = che
     def Avg(self):
        sum = self.math+self.phy+self.che
        avg = sum/3
        print(f"{self.name}, your average number is: {avg}")

     @staticmethod
     def hello():
       print("Hello World")

obj1 = Student ("Mafruda",90,89,97)
obj1.Avg()
obj1.hello()
